{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/auth/(.*)", "dest": "/auth.js" },
      { "src": "/properties/(.*)", "dest": "/listing.js" },
      { "src": "/bookings/(.*)", "dest": "/booking.js" },
      { "src": "/users/(.*)", "dest": "/user.js" },
      { "src": "/(.*)", "dest": "index.js" }
    ]
  }
  